{
    "name": "nuxt-app",
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=20.19.4"
    },
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare && prisma generate",
        "test": "vitest",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "db:generate": "prisma generate --config prisma.config.ts",
        "db:migrate": "prisma migrate dev --config prisma.config.ts",
        "db:reset": "prisma migrate reset --config prisma.config.ts",
        "db:studio": "prisma studio --config prisma.config.ts",
        "typecheck": "nuxt typecheck"
    },
    "dependencies": {
        "@nuxt/content": "^3.6.3",
        "@nuxt/image": "^1.11.0",
        "@nuxt/ui-pro": "3.3.0",
        "@pinia/colada": "^0.17.1",
        "@pinia/colada-nuxt": "^0.2.1",
        "@pinia/nuxt": "^0.11.2",
        "@prisma/client": "^6.14.0",
        "better-auth": "^1.3.4",
        "better-sqlite3": "^12.2.0",
        "date-fns": "^4.1.0",
        "nuxt": "4.0.3",
        "pinia": "^3.0.3",
        "vue": "3.5.18",
        "vue-router": "4.5.1",
        "zod": "^4.0.17"
    },
    "packageManager": "npm@10.9.2",
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@iconify-json/heroicons": "^1.2.2",
        "@nuxt/eslint": "^1.8.0",
        "@nuxt/test-utils": "^3.19.2",
        "@pinia/colada-devtools": "^0.1.5",
        "@vue/test-utils": "^2.4.6",
        "eslint": "^9.33.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "happy-dom": "^18.0.1",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "prisma": "^6.14.0",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.0",
        "vitest": "^3.2.4",
        "vue-eslint-parser": "^10.2.0",
        "vue-tsc": "3.0.5"
    }
}
